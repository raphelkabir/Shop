<html>
    <head>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        
        <!--Third-party resources-->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
        
        <!--Internal resources-->
        <link rel="stylesheet" href="/static/styles/sell.css">
        <script src="/static/scripts/sell.js"></script>
    </head>
    <body>
        <%-include('partials/navbar')%>
        <div class="container">
            <div class="container d-flex h-100 flex-column">
                <div class="flex-grow-1"></div>
                <div class="row justify-content-center">
                  <form id="form-product">
                    <div id="form-name" class="form-group">
                      <label for="product-name" class="form-label">Name</label>
                      <input id="product-name" class="form-control" placeholder="Product name">
                    </div>
                    <div id="form-description" class="form-group">
                      <label for="product-description" class="form-label">Description</label>
                      <input id="product-description" class="form-control" placeholder="Product description">
                    </div>
                    <div id="form-image" class="form-group">
                      <label for="product-image" class="form-label">Product image</label>
                      <input id="product-image" class="form-control" type="file">
                    </div>
                    <div id="form-price" class="form-group">
                      <label for="product-price" class="form-label">Price</label>
                      <input id="product-price" class="form-control" placeholder="Product price">
                    </div>
                    <div id="form-stock" class="form-stock">
                      <label for="product-stock" class="form-label">Stock</label>
                      <input id="product-stock" class="form-control" placeholder="Product stock">
                    </div>
                    <button id="btn-submit" type="submit" class="btn btn-primary">Sell</button>
                  </form>
                </div>
                <div class="flex-grow-1"></div>
              </div>
        </div>
    </body>
</html>